package arrayexample;

public class SquareOFSortedArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	}
	 public int[] sortedSquares(int[] A) {
		 int low=0, high=A.length;
		 if(A[0]<0) {
		 while(low<high) {
	    	 int mid=(low+high)/2;
	    	 if(A[mid]<0 && A[mid+1]>0) {
	    		 break;
	    	 }
	     }
		}
		    return null;
	        
	    }

}
